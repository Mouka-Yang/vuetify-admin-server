{
  "standard.enable": false,
  "files.eol": "\n",
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
